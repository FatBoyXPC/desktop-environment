#!/usr/bin/env bash

source "$(dirname "$0")/project-functions.sh"

cd ~/dev/perfectday/myshopmanager

switchphp 7

terminalOn "cd ~/Homestead;vagrant up;vagrant ssh" dev
terminalOn "fat-runner become" dev
terminalOn vim dev

terminalOn "less +F storage/logs/laravel.log" server

sudo systemctl start mariadb.service
terminalOn "mycli -u root auto_master" server
terminalOn "mycli -P 33060 -u homestead -p secret auto_master" server
